<!-- css -->
<link rel="stylesheet" href="/css/list-view-stadium.css">

<div class="bar-header"></div>
<div class="selection m-30">
    <div class="filter-area">
        <select name="type-filter" id="type-filter" class="input-custom f-1">
            <option value="None">All</option>
            <option value="0">Normal</option>
            <option value="1">VIP</option>
        </select>
        <button class="btn-custom f-1" id="filter">
            <i class="fa-solid fa-filter"></i>
        </button>
    </div>
    <div class="search-area">
        <input type="text" placeholder="Search by name" class="input-custom f-1" id="search">
        <button class="btn-custom f-1">
            <i class="fa-solid fa-magnifying-glass"></i>
        </button>
    </div>
</div>

<div class="list-stadiums" id="list-stadiums"></div>

<script src="/js/list-view-stadium.js"></script>
