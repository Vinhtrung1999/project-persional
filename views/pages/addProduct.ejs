<link rel="stylesheet" href="/css/addProduct.css">
<h2 class="text-center" style="padding: 10px;" id="head">Out put products warehouse</h2>

<div class="block-add f-1-2">
    <div class="block-add-item">
        <div class="block-add-item-title">ID Product</div>
        <div class="block-add-item-content">
            <input class="input-custom f-1-2" type="text" placeholder="Enter ID product" id="idPro" name="idPro" required>
        </div>
    </div>
    <div class="block-add-item">
        <div class="block-add-item-title">Quantity</div>
        <div class="block-add-item-content">
            <input class="input-custom f-1-2" type="number" placeholder="Enter quantity" id="qty" name="qty" required>
        </div>
    </div>

    <!-- button -->
    <div class="t-center">
        <button type="button" id="btn" class="btn-custom f-1-2 bg-green cl-white">
            <i class="fa-solid fa-plus"></i>
        </button>
        <div id="mess" class="success"></div>
        <div id="err" class="err"></div>
    </div>
</div>

<script src="/js/addProduct.js"></script>